{
  "JwtSettings": {
    "Key": "ThisIsASecretKeyForJwtToken", // Change to a strong secret key
    "Issuer": "MyCleanArchitectureApp", // Your issuer name
    "Audience": "MyCleanArchitectureAppUsers", // Your audience name
    "ExpiryMinutes": 60 // Token expiry time in minutes
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=MyCleanArchitectureAppDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AllowedHosts": "*"
}
